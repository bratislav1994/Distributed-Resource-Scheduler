<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <!-- LKResService  -->
      <service name="LKRes.Services.LKResService">
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:4000/ILKRes" />
          </baseAddresses>
        </host>
        <!-- Service Endpoints -->
        <endpoint address="" binding="netTcpBinding" contract="CommonLibrary.Interfaces.ILKRes" />
      </service>

      <!-- LKForClientService -->
      <service name="LKRes.Services.LKForClientService">
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:5000/ILKForClient" />
          </baseAddresses>
        </host>
        <!-- Service Endpoints -->
        <endpoint address="" binding="netTcpBinding" contract="CommonLibrary.Interfaces.ILKForClient" />
      </service>
    </services>
    
    <!--Client for KSRes-->
 
  </system.serviceModel>
</configuration>